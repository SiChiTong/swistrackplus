<?xml version="1.0"?>
<robosocietybuildercomponent>
	<friendlyname>Input ShopFloor Layout</friendlyname>
	<description>Set up a virtual factory.</description>
	<url></url>
	<displayimage>Output</displayimage>
	<configuration>
		<parameter type="string">
			<name>AssemblyAreaLabel</name>
			<label>Assembly Area </label>
			<default>Box Assembly</default>
			<reloadable>false</reloadable>
		</parameter>
		<parameter type="dropdownlist">
			<name>AssemblyAreaPlace</name>
			<label>Place</label>
			<items>
				<item key="17">Bottom1</item>
				<item key="18">Bottom2</item>
				<item key="19">Bottom3</item>
			</items>
			<reloadable>false</reloadable>
		</parameter>
		<parameter type="integer">
			<name>MaterialSources</name>
			<label>Material Sources</label>
			<min>1</min>
			<max>3</max>
			<default>3</default>
			<slider>true</slider>
		</parameter>
		<parameter type="string">
			<name>SourceLabel1</name>
			<label>1st. Part </label>
			<default>Base parts</default>
			<reloadable>false</reloadable>
		</parameter>
		<parameter type="dropdownlist">
			<name>SourcePlace1</name>
			<label>Place</label>
			<items>
				<item key="5">Left1</item>
				<item key="6">Left2</item>
				<item key="7">Left3</item>
			</items>
			<reloadable>false</reloadable>
		</parameter>
		<parameter type="string">
			<name>SourceLabel2</name>
			<label>2nd. Part </label>
			<default>Side parts</default>
			<reloadable>false</reloadable>
		</parameter>
		<parameter type="dropdownlist">
			<name>SourcePlace2</name>
			<label>Place</label>
			<items>
				<item key="9">Right1</item>
				<item key="10">Right2</item>
				<item key="11">Right3</item>
			</items>
			<reloadable>false</reloadable>
		</parameter>
		<parameter type="string">
			<name>SourceLabel3</name>
			<label>3rd. Part </label>
			<default>Top parts</default>
			<reloadable>false</reloadable>
		</parameter>
		<parameter type="dropdownlist">
			<name>SourcePlace3</name>
			<label>Place</label>
			<items>
				<item key="13">Top1</item>
				<item key="14">Top2</item>
				<item key="15">Top3</item>
			</items>
			<reloadable>false</reloadable>
		</parameter>	
				<parameter type="double">
			<name>BlockWidth</name>
			<label>BlockWidth</label>
			<min>10</min>
			<max>1000</max>
			<slider>true</slider>
			<reloadable>false</reloadable>
			<default>500</default>
			<sliderstep>10</sliderstep>
			<reloadable>false</reloadable>
		</parameter>
		<parameter type="double">
			<name>BlockHeight</name>
			<label>BlockHeight</label>
			<min>10</min>
			<max>1000</max>
			<slider>true</slider>
			<reloadable>false</reloadable>
			<default>500</default>
			<sliderstep>10</sliderstep>
			<reloadable>false</reloadable>
		</parameter>

	</configuration>
</robosocietybuildercomponent>
